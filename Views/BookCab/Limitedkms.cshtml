<script type="text/javascript">
    function AadharValidate() {

        var compare = document.getElementById("kal").value;
        if (compare == "Aadhar Number") {
            var aadhar = document.getElementById("txtAadhar").value;
            var adharcardTwelveDigit = /^\d{12}$/;
            var adharSixteenDigit = /^\d{16}$/;
            if (aadhar != '') {
                if (aadhar.match(adharcardTwelveDigit)) {
                    return true;
                }
                else if (aadhar.match(adharSixteenDigit)) {
                    return true;
                }
                else {
                    alert("Enter valid Aadhar Number");
                    return false;
                }
            }
        }
        else if (compare == "Passport Number") {
            var aadhar = document.getElementById("txtAadhar").value;
            var adharcardTwelveDigit = /^\d{8}$/;

            if (aadhar != '') {
                if (aadhar.match(adharcardTwelveDigit)) {
                    return true;
                }

                else {
                    alert("Enter valid Passport Number");
                    return false;
                }
            }
        }
        else {
            var aadhar = document.getElementById("txtAadhar").value;
            var adharcardTwelveDigit = /^\d{10}$/;

            if (aadhar != '') {
                if (aadhar.match(adharcardTwelveDigit)) {
                    return true;
                }

                else {
                    alert("Enter valid Voter Id ");
                    return false;
                }
            }
        }
    }
</script>
<script>
    var acc = document.getElementsByClassName("accordion");
    var i;

    for (i = 0; i < acc.length; i++) {
        acc[i].addEventListener("click", function () {
            this.classList.toggle("active");
            var panel = this.nextElementSibling;
            if (panel.style.display === "block") {
                panel.style.display = "none";
            } else {
                panel.style.display = "block";
            }
        });
    }
</script>
<script type="text/javascript">
    function LicenseValidate() {
        var aadhar = document.getElementById("txtLicense").value;

        var adharSixteenDigit = /[a-zA-Z]{2}[0-9]{14}$/;
        if (aadhar != '') {
            if (aadhar.match(adharSixteenDigit)) {
                return true;
            }

            else {
                alert("Enter valid License Number");
                return false;

            }
        }
    }
</script>

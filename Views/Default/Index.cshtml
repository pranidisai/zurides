<script>
    function comparetime() {
        var time1 = document.getElementById('timepicker1').value;
        var time2 = document.getElementById('timepicker2').value;
        var d = new Date(),
            h = d.getHours() + 2;
        m = d.getMinutes();
        var addhrs = h + ':' + m;
        if (time1 < addhrs || time2 < addhrs) {
            //alert("You can book at least 2 hours before pickup time.");
            document.getElementById('viewerr').innerHTML = "You can book at least 2 hours before pickup time.";
            var x = document.URL;
            //window.location.reload(x);
            window.location.href = x;
            //window.location.reload();

            return false;
        }

    }
</script>

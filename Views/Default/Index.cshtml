<style>
    .switch {
        background-color: #1bbc9b;
        width: 100%;
        display: inline-block;
        border-radius: 8px;
        height: 40px;
        padding: 1px;
        position: relative;
        zoom: 1;
    }

        .switch:before, .switch:after {
            content: " ";
            /* 1 */
            display: table;
            /* 2 */
        }

        .switch:after {
            clear: both;
        }

        .switch label {
            float: left;
            width: 50%;
            position: relative;
            z-index: 2;
            line-height: 40px;
            cursor: pointer;
            color: #117964;
        }

        .switch input[type="radio"] {
            display: none;
        }

            .switch input[type="radio"]:checked + label {
                color: #141241;
                font-weight: bold;
            }

        .switch span {
            position: relative;
            z-index: 1;
            top:;
            left: 0px;
            width: 50%;
            height: 100%;
            display: block;
            border-radius: 5px 5px 5px 5px;
            background-color: #ffffff;
            transition: all 200ms ease;
        }

            .switch span.right {
                top: -45px;
                width: 50%;
                left: 0%;
                border-radius: 5px 5px 5px 5px;
                transition: all 200ms ease;
            }
</style>

<style>
    input[type="text"],
    select.form-control {
        border: 1px solid grey;
        color: black;
        font-weight: bold;
        font-size: 30px;

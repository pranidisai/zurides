<script>
    $(window).on('load', function () {
        document.getElementById("optionone").checked = true;
        document.getElementById("optiontwo").checked = false;
    })

    function Validate() {
        if (document.getElementById("optionone").checked == true) {

            $("#txt_soruceLocation").prop('required', true);
        }
        if (document.getElementById("optiontwo").checked == true) {

            $("#txt_soruceLocation").prop('required', true);
            $("#txt_DestinationLocation").prop('required', true);
        }
    }

</script>
<script type="text/javascript">

    $(document).ready(function () {
        var sourcecity = document.getElementById('City');
        $("#City").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/Default/AutoCompleteCountry",
                    type: "GET",
                    dataType: "json",
                    data: { term: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return { label: item.City, value: item.City };
                        }))
                    }
                })
            },
            messages: {
                noResults: "", results: ""
            }
        });
    })

</script>
<script>
    $('.switch label').on('click', function () {
        var indicator = $(this).parent('.switch').find('span');
        if ($(this).hasClass('right')) {
            $(indicator).addClass('right');
        } else {
            $(indicator).removeClass('right');
        }
    });
</script>
<script>
    $(document).ready(function () {
        $("#k1").click(function () {
            $("#dest").hide();
        });
    });

    $(document).ready(function () {
        $("#k2").click(function () {
            $("#dest").show();

        });
    });
</script>

